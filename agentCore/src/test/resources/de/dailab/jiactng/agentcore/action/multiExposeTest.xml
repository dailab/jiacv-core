<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="classpath:de/dailab/jiactng/agentcore/conf/AgentNode.xml" />
    <import resource="classpath:de/dailab/jiactng/agentcore/conf/JMSMessaging.xml" />
    <import resource="classpath:de/dailab/jiactng/agentcore/conf/Agent.xml" />

    <bean name="MultiExposeNode" parent="NodeWithJMX">
        <property name="agents">
            <list>
			    <bean name="MultiExposeAgent" parent="SimpleAgent">
					<property name="agentBeans">
						<list>
							<bean class="de.dailab.jiactng.agentcore.action.ExposeMultiReturnBean" scope="prototype" />
						</list>
					</property>
				</bean>
            </list>
        </property>
    </bean>
    
</beans>

