<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<import resource="classpath:de/dailab/jiactng/agentcore/conf/Agent.xml" />


	<bean name="LoadReporterAgent" parent="NonBlockingAgent">
		<property name="agentBeans">
			<list>
				<ref bean="LoadReporterMethodExposingBean" />
				<ref bean="SchedulerMethodExposingBean" />
				<ref bean="SchedulerTestBean" />
				<ref bean="DirectoryAccessBean" />
				<ref bean="CommunicationBean" />
				</list>
			</property>
		</bean>

</beans>
