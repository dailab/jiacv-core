<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<import resource="classpath:de/dailab/jiactng/agentcore/conf/AgentNode.xml" />
	<import resource="classpath:de/dailab/jiactng/agentcore/conf/Agent.xml" />
	
	<bean name="NodeConfigurationMonitorTestPlatform" parent="Platform">
		
		<property name="agents">
			<list>
				</list>
			</property>
		
		<property name="agentNodeBeans">
			<list>
				<ref bean="NodeConfigurationMonitorBean"/>
				</list>
			</property>
		
		</bean>
	
	<bean name="NodeConfigurationMonitorBean" class="de.dailab.jiactng.agentcore.conf.NodeConfigurationMonitorBean" singleton="false">
		<property name="logLevel" value="DEBUG"/>
		</bean>
	
	</beans>
