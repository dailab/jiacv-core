<configuration xmlns="http://www.jiac.de/AAMM/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.jiac.de/AAMM/1.0 http://www.jiac.de/xsd/aamm-1.0.xsd"
	framework="JIAC V">
	
	<node id="NodeWithRemoteJMX">
		<parent namespace="de.dailab.jiactng.agentcore.aamm.default" ref="Node"/>
		<list name="jmxConnectors">
			<objItem class="de.dailab.jiactng.agentcore.management.jmx.RmiJmxConnector">
				<object name="authenticator" class="de.dailab.jiactng.agentcore.management.jmx.PasswordFileAuthenticator">
					<simple name="passwordResource" value="de/dailab/jiactng/agentcore/management/jmx/passwords.properties"/>
				</object>
			</objItem>
		</list>
	</node>
</configuration>